<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
#ifdef FREEVERSION
  xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads" 
#endif
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:orientation="vertical">

#ifdef FREEVERSION
	<com.google.ads.AdView android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        ads:adUnitId="a14de22b29b03f5"
        ads:adSize="BANNER"
        ads:loadAdOnCreate="true"
        />
#endif

<LinearLayout
 android:layout_width="fill_parent"
 android:layout_height="wrap_content"
 android:orientation="horizontal">
  
  <ToggleButton android:text="ON"
   android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:id="@+id/eq_onoff"></ToggleButton>
  <Button android:id="@+id/preset" android:text="Presets" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
  <Button android:id="@+id/eqreset" android:text="All zero" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>


</LinearLayout>

<ScrollView android:id="@+id/ScrollView01" android:layout_width="fill_parent" android:layout_height="wrap_content">

<LinearLayout 
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:orientation="vertical">

