	<SeekBar android:id="@+id/preamp" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1" android:paddingBottom="10sp" />
	<TextView android:layout_width="55sp" android:layout_height="wrap_content" android:gravity="right" android:text="+0 dB" android:id="@+id/db_preamp"></TextView>
</LinearLayout>

