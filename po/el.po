#: ../main.c:92
#, c-format
msgid "Options:\n"
msgstr "Επιλογές:\n"

#: ../main.c:93
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr " --help ή -h Εμφάνιση κειμένου βοήθειας και έξοδος\n"

#: ../main.c:94
#, c-format
msgid "   --quit             Quit player\n"
msgstr "--quit Έξοδος\n"

